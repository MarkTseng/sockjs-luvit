/* Lambda calculus grammar by Zach Carter */

%lex

%%

\s+      {/* ignore */}
"/**"    { return 'BEGIN'; }
"**/"    { return 'END'; }
.*       { return 'TEXT'; }

/lex

%%

input
  :    /* empty */
      | input BEGIN comment END
  ;

comment
  : TEXT { console.log("FOUND: %s\n", $$) }
  ;







  /** read-only
   *  Application#root -> String
   *
   *  Applications's root directory
   **/
  this.__defineGetter__('root', function get_root() {
    return options.root;
  });


  /** read-only
   *  Application#name -> String
   *
   *  Applications's name.
   **/
  this.__defineGetter__('name', function get_name() {
    return name;
  });
